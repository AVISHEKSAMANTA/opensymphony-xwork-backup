<xface:tab
	xmlns:xface="http://www.xesoft.com/xmlns/xface"
	xmlns:xprocess="http://www.xesoft.com/xmlns/xprocess"
	id="result_types">

	<xface:relative-layout constraints="/xwork-editor/xwork-panel-right2.layout.xml"/>

	<xface:toolbar id="toolbar">
		<xface:toolbar-button id="create_result_type" image="xwork-editor/images/add_result.gif"/>
		<xface:toolbar-button id="delete_result_type" image="xwork-editor/images/delete.gif">
			<xface:selected-list-enabled-adapter element="../../../table" selection="both"/>
		</xface:toolbar-button>
	</xface:toolbar>

	<xface:table id="table">
		<xface:xml-tablemodel id="model" xpath="result-type"/>
		<xface:column id="name" property="@name"/>
		<xface:column id="class" property="@class"/>
	</xface:table>

	<!--
		Panel Includes
   -->
	<xface:include src="createresulttype.xfm.xml" />
	<xface:include src="deleteresulttype.xfm.xml" />

</xface:tab>